{
  "name": "flyin-awesome-monorepo",
  "version": "1.0.0",
  "description": "",
  "workspaces": [
    "packages/*",
    "apps/*",
    "platforms/*"
  ],
  "main": "index.js",
  "scripts": {
    "dev": "pnpm --filter portal run dev",
    "build": "pnpm --filter portal run build",
    "dev:js": "pnpm --filter vue-js-app run dev",
    "build:js": "pnpm --filter vue-js-app run build",
    "dev:ts": "pnpm --filter vue-ts-app run dev",
    "build:ts": "pnpm --filter vue-ts-app run build",
    "dev:react": "pnpm --filter react-ts-app run dev",
    "build:react": "pnpm --filter react-ts-app run build",
    "dev:all": "pnpm --parallel --filter \"./apps/*\" run dev",
    "build:all": "pnpm --filter \"./apps/*\" run build",
    "build:shared": "pnpm --filter \"./packages/shared-utils\" run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
  },
  "keywords": [],
  "author": "wenqiu <liming20110711@163.com>",
  "license": "ISC",
  "packageManager": "pnpm@10.14.0"
}
